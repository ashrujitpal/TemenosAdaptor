                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attribute default="PROCESS" name="operation" type="ofsmlPVBOperationCode"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlTransactionAuthoriseRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a transaction authorise service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="transactionId" type="globusTransactionId"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attribute default="PROCESS" name="operation" type="ofsmlPVOperationCode"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlTransactionSeeRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a transaction see (show) service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="transactionId" type="globusTransactionId"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attribute fixed="PROCESS" name="operation" type="ofsmlPVOperationCode"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlTransactionReverseRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a transaction reverse service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="transactionId" type="globusTransactionId"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attribute default="PROCESS" name="operation" type="ofsmlPVOperationCode"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlTransactionDeleteRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a transaction delete service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="transactionId" type="globusTransactionId"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
