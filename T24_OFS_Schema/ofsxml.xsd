        <xsd:complexType final="#all" name="ofsmlTransactionOfflineResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes an off-line transaction response (rejected due to GLOBUS running the close of business
                                cycle - this option is configurable in GLOBUS).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="transactionId" nillable="true" type="globusTransactionId"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="field" type="ofsmlMVField"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attributeGroup ref="ofsmlTransactionDirective"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlTransactionQueuedResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes an off-line queued transaction response (accepted for processing at the start of the next
                                business cycle).  An off-line reference is returned.  Where a transaction id was specified, this is returned also
                                (otherwise a nil transactionId element is returned).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="offlineReference" type="globusOfflineReference"/>
                        <xsd:element name="transactionId" nillable="true" type="globusTransactionId"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="field" type="ofsmlMVField"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attributeGroup ref="ofsmlTransactionDirective"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlTransactionFailedResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a failure to process the transaction due to a business validation failure.  The original
                                transaction request is returned, along with the fields in error.  If the GTS Control is configured (in GLOBUS) to
                                accept failed transactions to the 'hold' work queue, the held attribute is set to true and the associated transaction
                                ID reserved by GLOBUS is returned in the optional transactionId element.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element minOccurs="0" name="transactionId" nillable="true" type="globusTransactionId"/>
                        <xsd:element maxOccurs="unbounded" name="error" type="ofsmlErrorField"/>
                        <xsd:group ref="ofsml12TransactionRequests"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attributeGroup ref="ofsmlTransactionDirective"/>
                <xsd:attribute default="false" name="held" type="xsd:boolean"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlStandardEnquiryRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2 model.  Denotes a standard (normalised) enquiry service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="selectionCriteria" type="ofsmlSelectionCriteria"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusEnquiryName" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlExtendedEnquiryRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a de-normalised (extended) enquiry service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="selectionCriteria" type="ofsmlSelectionCriteria"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusEnquiryName" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlStandardEnquiryResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a standard (normalised) enquiry service response.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryHeader" type="xsd:string"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryColumn" type="ofsmlEnquiryColumnDefinition"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryRecord" type="ofsmlStandardEnquiryRecord"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusEnquiryName" use="required"/>
                <xsd:attribute name="status" type="ofsmlEnquiryStatus" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlExtendedEnquiryResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a de-normalised (extended) enquiry service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryHeader" type="xsd:string"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryColumn" type="ofsmlEnquiryColumnDefinition"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryRecord" type="ofsmlExtendedEnquiryRecord"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusEnquiryName" use="required"/>
                <xsd:attribute name="status" type="ofsmlEnquiryStatus" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlStandardRoutineRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes a standard (non-XML) custom routine service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="parameter" type="ofsmlRoutineParameter"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusRoutineName" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlXMLRoutineRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  Denotes an XML custom routine service request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:any minOccurs="0" namespace="##other" processContents="lax"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusRoutineName" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlStandardRoutineResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  An OFSML (standard) routine response is the result of a service request to execute a custom routine in the GLOBUS
                                environment where the routine is producing string-based output.  No validation of the output is performed, but forbidden characters are
                                replaced with escape sequences.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attribute name="name" type="globusRoutineName" use="required"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlXMLRoutineResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model.  An OFSML (XML) routine response is the result of a service request to execute a custom routine in the GLOBUS
                                environment where the routine is directly producing an XML output compliant with the GlobusRoutineResponse element definition.
                                The type (GLOBUS name) of the routine is denoted by the attributes.  Where an external element definition is utilised, XML parsing errors
                                will cause an ofsFault element to be returned (XML validation processing is set to lax; a content model should be supplied but is not
                                mandatory).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:any minOccurs="0" namespace="##other" processContents="lax"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusRoutineName" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlBusinessEvent">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                OFSML 1.2+ model group.  Denotes a business event, generated as a result of an action within GLOBUS.  The source and type of
                                the event are indicated by the associated attributes.  A business event is a generic representation of the state and content of any transaction.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="eventPublished" type="xsd:dateTime"/>
                        <xsd:element name="eventTransmitted" type="xsd:dateTime"/>
                        <xsd:element minOccurs="0" name="eventExpiry" type="xsd:duration"/>
                        <xsd:element name="eventId" type="ofsmlMessageReference"/>
                        <xsd:element minOccurs="0" name="status" type="ofsmlProcessingStatus"/>
                        <xsd:element minOccurs="0" name="publishingUser" type="globusUserName"/>
                        <xsd:element minOccurs="0" name="transactionId" type="globusTransactionId"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="field" type="ofsmlMVField"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="override" type="ofsmlOverrideField"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="auxField" type="ofsmlMVField"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="auxOverride" type="ofsmlOverrideField"/>
                </xsd:sequence>
                <xsd:attribute name="source" type="ofsmlEventSource" use="required"/>
                <xsd:attribute name="type" type="ofsmlEventType" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="restriction" name="ofsmlMVField">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A multi-value field definition.  Note that the multi-value and sub-value attributes are optional; where not present the element may be
                                used to represent a normal (non multi-value) field.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attributeGroup ref="ofsmlFieldIdentifier"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlOverrideField">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                An override condition from GLOBUS, identifying the override condition and containing an optional override code.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attribute name="code" type="globusOverrideCode"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlErrorField">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                An error condition from GLOBUS, identifying a field in error, the error condition and containing an optional error code.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attributeGroup ref="ofsmlFieldIdentifier"/>
                                <xsd:attribute name="code" type="globusErrorCode"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlSelectionCriteria">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A selection criteria provided by the caller when requesting information from GLOBUS via an enquiry.  Uses a
                                Reverse-Polish style argument order.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="fieldName" type="globusFieldName"/>
                        <xsd:element maxOccurs="unbounded" name="value" type="globusFieldValue"/>
                </xsd:sequence>
                <xsd:attribute name="operand" type="globusSelectionOperand" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlEnquiryColumnDefinition">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                An enquiry column definition provided in an enquiry response.  Note the default for the globusType attribute
                                (the type of the column, as returned from GLOBUS) is an implicit GLOBUS string.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:token">
                                <xsd:attribute name="id" type="globusFieldName" use="required"/>
                                <xsd:attribute name="label" type="globusColumnLabel" use="required"/>
                                <xsd:attribute name="globusType" type="globusDataType"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlStandardEnquiryRecord">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A single record returned by a standard enquiry response from GLOBUS.  Consists of a set of values that relate directly
                                (correlated by the position index in a standard response) to the order of the enquiry column definitions of the related
                                GLOBUS enquiry.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="column" type="ofsmlStandardColumn"/>
                </xsd:sequence>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlExtendedEnquiryRecord">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A single record returned by an extended enquiry response from GLOBUS.  Consists of a set of values that relate directly
                                (correlated by the id attribute) to the the enquiry column definitions of the related GLOBUS enquiry.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="column" type="ofsmlExtendedColumn"/>
                </xsd:sequence>
        </xsd:complexType>
        <xsd:complexType name="ofsmlExtendedColumn">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The value of a column in an OFSML extend enquiry response.  The id attribute is used to denote the associated GLOBUS
                                column identifier.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attribute name="id" type="globusFieldName" use="required"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" mixed="true" name="ofsmlFaultMessage">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Where a (technical) failure to process a request has occurred, the fault message may be returned to help determine the cause.
                                Note that the fault message may contain both a text error message, plus an optional (tagged) GLOBUS error code (for extensibility).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element minOccurs="0" name="globusErrorCode" nillable="true" type="globusErrorCode"/>
                </xsd:sequence>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlFaultDetails">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Where a technical failure to process a request has occurred, the fault details may be returned to help determine the cause
                                of the error.  The default is to return the original service request, but custom routines may return their own fault details
                                where required via the GlobusRoutineResponse wrapper element.   Where an external element definition is utilised,
                                parsing errors will cause a default ofsFault (XML parsing fault).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element minOccurs="0" name="securityContext" type="ofsmlSecurityContext"/>
                        <xsd:element minOccurs="0" name="duplicateReference" nillable="true" type="ofsmlFaultDuplicateTrnRef"/>
                        <xsd:choice>
                                <xsd:group ref="ofsml12ServiceRequests"/>
                                <xsd:group ref="ofsml11ServiceRequests"/>
                                <xsd:any namespace="##other" processContents="lax"/>
                        </xsd:choice>
                </xsd:sequence>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlFaultDuplicateTrnRef">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Used to denote the reference of a previously processed transaction (held in the target GLOBUS database) where the unique message reference (UMR)
                                matches the UMR on the current message (a fault condition).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:token">
                                <xsd:attributeGroup ref="ofsmlTransactionOptionalType"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsmlRoutineParameter">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A parameter used in the invocation of a custom routine.  A specific delimiter, to precede the next parameter element instance,
                                may be specified via the delimiter attribute.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attribute default="," name="delimiter" type="ofsmlParameterDelimiter"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11TransactionRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  An OFSML transaction denotes a request by the caller to execute a specific GLOBUS application or version.  The
                                attributes denote the transaction type (the GLOBUS unique identity of the application/version).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element name="ofsDirective" type="ofsml11TransactionDirective"/>
                        <xsd:element minOccurs="0" name="transactionId" nillable="true" type="globusTransactionId"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="field" type="ofsmlMVField"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="auxField" type="ofsmlMVField"/>
                </xsd:sequence>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11TransactionResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  An OFSML transaction response is the result of a service request to execute a transaction (GLOBUS application or version).
                                The transaction response contains all information pertaining to the execution of the transaction, including two levels of status information,
                                transaction type and the associated processing directive, transaction id or off-line reference and all transaction data (fields, overrides and/or
                                processing errors).  See the OFSML use cases for examples of transaction processing scenarios.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:choice>
                        <xsd:sequence>
                                <xsd:element name="ofsDirective" nillable="true" type="ofsml11TransactionDirective"/>
                                <xsd:element name="processingStatus" type="ofsml11ProcessingStatus"/>
                                <xsd:element minOccurs="0" name="auxProcessingStatus" type="ofsml11ProcessingStatus"/>
                                <xsd:choice>
                                        <xsd:element name="transactionId" nillable="true" type="globusTransactionId"/>
                                        <xsd:element name="offlineReference" nillable="true" type="globusOfflineReference"/>
                                </xsd:choice>
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="field" type="ofsmlMVField"/>
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="override" type="ofsmlOverrideField"/>
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="auxField" type="ofsmlMVField"/>
                                <xsd:element maxOccurs="unbounded" minOccurs="0" name="auxOverride" type="ofsmlOverrideField"/>
                        </xsd:sequence>
                        <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="error" type="ofsmlErrorField"/>
                                <xsd:element name="ofsTransaction" type="ofsml11TransactionRequest"/>
                        </xsd:sequence>
                </xsd:choice>
                <xsd:attributeGroup ref="ofsmlTransactionType"/>
                <xsd:attribute name="status" type="ofsml11TransactionStatus" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11EnquiryRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  An OFSML enquiry denotes a request by the caller to execute a GLOBUS enquiry.  The sole member is the
                                set of optional selection criteria for the enquiry.  Note that the operand applied across multiple selectionCriteria elements is an implicit logical
                                AND.  The type of the enquiry is denoted by the attributes (the GLOBUS name for the enquiry).  The response format may be requested as
                                a normalised or de-normalised grid via the responseType attribute (default is normalised).  NOTE: only grid-type enquiries are supported;
                                targetting non-grid enquiries will result in malformed XML responses with a standard format and an ofsFault in extended format.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="selectionCriteria" type="ofsmlSelectionCriteria"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusEnquiryName" use="required"/>
                <xsd:attribute default="STANDARD-OFSML" name="responseType" type="ofsml11EnquiryFormat"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11EnquiryResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  An OFSML enquiry response is the result of a service request to execute an enquiry in the
                                GLOBUS environment.  It consists of a mandatory status, an optional set of enquiry headers (text) and an optional set of enquiry
                                column definitions followed by one or more enquiry records (containing the column values, by row).  The type (GLOBUS
                                unique name) of the enquiry is denoted by the attributes, plus the return status of executing the enquiry.
                                The attributes also denote the type of the response (standard or extended OFSML, where extended indicates the
                                enquiry definition data is partially de-normalised, with column elements referencing the enquiry column id via attributes).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryHeader" type="xsd:string"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryColumn" type="ofsmlEnquiryColumnDefinition"/>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="enquiryRecord" type="ofsml11EnquiryRecord"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusEnquiryName" use="required"/>
                <xsd:attribute name="status" type="ofsmlEnquiryStatus" use="required"/>
                <xsd:attribute name="responseType" type="ofsml11EnquiryFormat" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11RoutineRequest">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  An OFSML routine denotes a request by the caller to execute a custom routine in the GLOBUS environment.
                                The routine type (the GLOBUS name for the routine) is denoted by the attributes.  Note that the output of a routine may be structured (XML)
                                or unstructured (for direct parsing and/or interpretation by the caller).  Where routines are accepting/returning an XML document
                                fragment, they must conform to the guidelines for use of the GlobusRoutineResponse element.  For normal (unstructured) use, an
                                arbitrary number of parameters may be passed to the routine, where the default parameter delimiter (to be used by the routine) is
                                denoted by the associated attribute.  If no delimiter is specified, the default is a single semi-colon. For XML (structured) use, both input
                                and output are restricted to a single root element of an XML document fragment.  See the OFSML use cases for examples of
                               custom routine invocations and responses.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:choice minOccurs="0">
                                <xsd:element maxOccurs="unbounded" name="parameter" type="ofsmlRoutineParameter"/>
                                <xsd:element name="xParameter" type="ofsml11RoutineXMLParameter"/>
                        </xsd:choice>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusRoutineName" use="required"/>
        </xsd:complexType>
        <xsd:complexType mixed="true" name="ofsml11RoutineResponse">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  An OFSML routine response is the result of a service request to execute a custom routine in the GLOBUS
                                environment.  The response consists of a single mandatory member detailing the output of the routine.  The output of a routine may be
                                unstructured (the default), requiring that the caller is explicitly aware of the return format, or structured to support an agreed
                                XML Schema between the target routine and the caller.  In the case of a routine returning XML, the routine's output is treated as
                                a document fragment and must utilise the wrapper element GlobusRoutineResponse for it's return value.  The type (GLOBUS
                                name of the routine) of the routine is denoted by the attributes.  Where an external element definition is utilised, XML parsing errors
                                will cause an ofsFault element to be returned (XML validation processing is set to strict; a content model must be supplied).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:any minOccurs="0" namespace="##other" processContents="strict"/>
                </xsd:sequence>
                <xsd:attribute name="name" type="globusRoutineName" use="required"/>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11TransactionDirective">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  The processing directive of a transaction service request to GLOBUS/OFS.  Indicates the
                                function and operation for a transaction request.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:token">
                                <xsd:attributeGroup ref="ofsmlTransactionDirective"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" name="ofsml11EnquiryRecord">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  A single record returned by an enquiry response from GLOBUS.  Consists of a set of values that
                                relate directly (correlated by the position index in a standard response or the column id attribute in an extended response) to the enquiry
                                column definition of the related GLOBUS enquiry.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:choice>
                        <xsd:element maxOccurs="unbounded" name="column" type="ofsml11Column"/>
                </xsd:choice>
        </xsd:complexType>
        <xsd:complexType name="ofsml11Column">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  The value of a column in an OFSML enquiry response.  The id attribute is used to denote the associated GLOBUS
                                column identifier in the column definitions only where an extended (OFSML) response is requested.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                                <xsd:attribute name="id" type="globusFieldName"/>
                        </xsd:extension>
                </xsd:simpleContent>
        </xsd:complexType>
        <xsd:complexType final="#all" mixed="true" name="ofsml11RoutineXMLParameter">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  A parameter used in the invocation of a custom routine accepting an XML document fragment as input. Any
                                arbitrary XML document fragment may be specified in the instance document.  Where an external element definition is utilised, parsing errors
                                will cause a default ofsFault to be generated (XML parsing fault).  Note that the input is restricted to a single document fragment root.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:sequence>
                        <xsd:any namespace="##other" processContents="lax"/>
                </xsd:sequence>
        </xsd:complexType>

        <!-- SIMPLE DATATYPE DEFINITIONS -->

        <xsd:simpleType name="ofsmlSignatureDirective">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Used to control the presence of a digital signature on OFSML document instance.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="NONE"/>
                        <xsd:enumeration value="BOTH"/>
                        <xsd:enumeration value="REQUEST-ONLY"/>
                        <xsd:enumeration value="RESPONSE-ONLY"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlFireAndForgetDirective">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Directive to instruct the GC how to manage the request/response semantics.
                                NEVER: never use fire and forget mode (dispose of all responses).
                                ALWAYS: always use fire and forget mode (do not dispose of reponses).
                                ON-FAIL: use fire and forget only for processing failures (dispose failed responses).
                                ON-SUCCESS: use fire and forget only for successfully processed messages (dispose successful responses).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="NEVER"/>
                        <xsd:enumeration value="ALWAYS"/>
                        <xsd:enumeration value="ON-FAILURE"/>
                        <xsd:enumeration value="ON-SUCCESS"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlTranslatorContext">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The context of the response translation request, which may be 'local processing' (performed by the GLOBUS Connector)
                                or 'deferred' to another service (typically the caller).  Local HTML rendering of XML documents may be requested also, for
                                improved message readability via display in standard Web Browser applications.
                                LOCAL: Apply the transform specified to the OFSML output and return the tranformed document.
                                DEFERRED: This is intended for audit purposes only, to allow a caller to inform all actors that the response message will be transformed.
                                HTML-RENDER: Apply the stylesheet (URI) reference to the OFSML response message.  This is for easier viewing of messages
                                from browsers that support W3C compliant stylesheet processing.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="LOCAL"/>
                        <xsd:enumeration value="DEFERRED"/>
                        <xsd:enumeration value="HTML-RENDER"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlFaultCode">
                <xsd:annotation>
                        <xsd:documentation>
                                In the event of a (technical) failure to process a request (i.e. the request could not be handled), the error condition may be signalled
                               via one of the fault codes.  The 'recoverable' attribute denotes if the request may be retried without repair by the caller.
                                Note that in the event of a response (a processed message) reaching it's expiry time, any subsequent compensating action is the
                               responsibility of the caller.
                                DUPLICATE-MESSAGE: The UMR (correlation id) specified has been used on a previous message (recoverable = "true").
                                EVENT-SOURCE-INVALID: The event source requested is not defined in the system (recoverable = "false").
                                EVENT-TYPE-INVALID: The event type requested is not defined in the system (recoverable = "false").
                                GLOBUS-UNAVAILABLE: Sessions to GLOBUS are unavailable or disabled (recoverable = true).
                                INVALID-RECORD: The requested record does not exist in the GLOBUS database (recoverable = false).
                                MAX-EXTERNAL-RETRY: The maximum retry attempts was reached, as defined in the OFSML instance document (recoverable = true).
                                RECORD-LOCKED: The target record (in GLOBUS) was locked (recoverable = true).
                                REQUEST-EXPIRED: The request expiry time was reached before it was submitted for processing (recoverable = true).
                                REQUEST-INVALID: The request (type) was recognised by GLOBUS but not well-formed e.g. incorrect field names (recoverable = false).
                                REQUEST-TIMEOUT: The request time-out period was reached during processing (recoverable = true).
                                SECURITY-VIOLATION: GLOBUS reported the specified userid is not recognised or is not permitted to execute the requested service (recoverable = true).
                                UNDEFINED-FATAL-ERROR: An unknown error occurred (recoverable = false).
                                UNRECOGNISED-REQUEST: The request (type) was not recognised by GLOBUS e.g. incorrect application name (recoverable = false).
                                VOID-RESPONSE: GLOBUS returned a void response - typically implies the request was not processed due to technical errors, for example in the
                                UniVerse API (recoverable = true).
                                XML-PARSING-ERROR: Where an error occurred parsing an OFSML message (not a valid XML document) - typically, this only occurs
                                where custom routines return XML document fragments using the 'wildcard' substitution or where a native asynchronous message bus
                               communications channel is being utilised (recoverable = false).
                                XML-VALIDATION-ERROR: Where an XML document validation error occurred parsing an OFSML message (valid XML but invalid
                                schema instance) - typically, this only occurs when custom routines return XML document fragments using the 'wildcard' substitution
                                and provide an external schema to validate their document fragments,  or where a native asynchronous message bus
                               communications channel is being utilised (recoverable = false).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="DUPLICATE-MESSAGE"/>
                        <xsd:enumeration value="EVENT-SOURCE-INVALID"/>
                        <xsd:enumeration value="EVENT-TYPE-INVALID"/>
                        <xsd:enumeration value="GLOBUS-UNAVAILABLE"/>
                        <xsd:enumeration value="INVALID-RECORD"/>
                        <xsd:enumeration value="MAX-EXTERNAL-RETRY"/>
                        <xsd:enumeration value="RECORD-LOCKED"/>
                        <xsd:enumeration value="REQUEST-EXPIRED"/>
                        <xsd:enumeration value="REQUEST-INVALID"/>
                        <xsd:enumeration value="REQUEST-TIMEOUT"/>
                        <xsd:enumeration value="SECURITY-VIOLATION"/>
                        <xsd:enumeration value="UNDEFINED-FATAL-ERROR"/>
                        <xsd:enumeration value="UNRECOGNISED-REQUEST"/>
                        <xsd:enumeration value="VOID-RESPONSE"/>
                        <xsd:enumeration value="XML-PARSING-ERROR"/>
                        <xsd:enumeration value="XML-VALIDATION-ERROR"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlPVBOperationCode">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The GLOBUS directive for handling any transaction processing request.
                                PROCESS: Directs GLOBUS to fully processes the message and commit any updates to the database.
                                VALIDATE: Directs GLOBUS to simulate the full processing of the message to include all validation but not commit any
                                updates to the database.  This returns all default values (only for fields where default values are present).
                                BUILD: Directs GLOBUS to attempt to build a transaction.  Field values are defaulted where available and field values supplied
                                are used in building the transaction data.  Returns a status to indicate the validity of the built transaction.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="PROCESS"/>
                        <xsd:enumeration value="VALIDATE"/>
                        <xsd:enumeration value="BUILD"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlPVOperationCode">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The GLOBUS directive for handling any transaction processing request.
                                PROCESS: Directs GLOBUS to fully processes the message and commit any updates to the database.
                                VALIDATE: Directs GLOBUS to simulate the full processing of the message to include all validation but not commit any
                                updates to the database.  This returns all default values (only for fields where default values are present).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="PROCESS"/>
                        <xsd:enumeration value="VALIDATE"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlFunctionCode">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The standard function code of any transaction procesing request to GLOBUS.  Function codes
                                are: 'INPUT' - input/insert the transaction; 'SEE' - see (show) the transaction (requires a transactionId); 'AUTHORISE' - authorise
                                the transaction;  'REVERSE' - reverse (unwind) the transaction; 'DELETE' - delete the transaction.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="INPUT"/>
                        <xsd:enumeration value="SEE"/>
                        <xsd:enumeration value="AUTHORISE"/>
                        <xsd:enumeration value="REVERSE"/>
                        <xsd:enumeration value="DELETE"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlProcessingStatus">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Represents an extended processing return code, mapped from RECORD.STATUS (if not present, a status = OK is assumed).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="OK"/>
                        <xsd:enumeration value="REVERSED"/>
                        <xsd:enumeration value="DELETED"/>
                        <xsd:enumeration value="NOT-AUTHORISED"/>
                        <xsd:enumeration value="ON-HOLD"/>
                        <xsd:enumeration value="MATURED"/>
                        <xsd:enumeration value="PARTIAL-INPUT"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusSelectionOperand">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The standard operands used in a selection criteria when requesting grid-based (enquiry) information from GLOBUS.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="EQ"/>
                        <xsd:enumeration value="NE"/>
                        <xsd:enumeration value="GE"/>
                        <xsd:enumeration value="GT"/>
                        <xsd:enumeration value="LE"/>
                        <xsd:enumeration value="LT"/>
                        <xsd:enumeration value="UL"/>
                        <xsd:enumeration value="LK"/>
                        <xsd:enumeration value="NR"/>
                        <xsd:enumeration value="RG"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlEnquiryStatus">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The return status from executing an enquiry.
                               OK: The enquiry request resulted in a data set.
                               NO-RECORDS: The enquiry request resulted in a null data set.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="OK"/>
                        <xsd:enumeration value="NO-RECORDS"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlRoutineStatus">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The return status from executing a (custom) routine where the routine returns the XML document fragment
                                defined by complex type GlobusRoutineResponse.
                               OK: The routine reported it executed correctly - the response content will be mapped to the routineResponse element.
                               FAILED: The routine reported an execution failure - the response content will be mapped to an ofsFault element.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="OK"/>
                        <xsd:enumeration value="FAILED"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusDataType">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The GLOBUS datatype for a field.  If not specified, the default type (in GLOBUS) is 'STRING'.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="STRING"/>
                        <xsd:enumeration value="NUMERIC"/>
                        <xsd:enumeration value="AMOUNT"/>
                        <xsd:enumeration value="DATE"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlMessageReference">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A request or response message reference.  The GLOBUS Connector generates UUIDs for all responses
                                and for requests where the GC APIs are used, or utilises the native message transport IDs where available.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="12"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlChannelName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The unique name of a (technical) communications channel to GLOBUS (via the GLOBUS Connector).
                                Note that channel names must be a minimum of 4 characters long in an OFSML document.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="3"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlStandardColumn">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The value of a column in an OFSML standard enquiry response.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusUserName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A unique GLOBUS user name.  May represent a unique user, an interface daemon, business channel or an external application.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusUserPassword">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A GLOBUS user password.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusCompanyId">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The GLOBUS name that uniquely identifies a company.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token"/>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlNickName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A simple representation of a callers security credentials.  Used by a security sub-system only, for example, for passing a session id.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="8"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlParameterDelimiter">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Used to indicate a single-character delimiter used to separate parameters in a call to GLOBUS.  This
                                is only used for the invocation of custom routines.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:maxLength value="1"/>
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlEventSource">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Used to indicate an event source (the publishing entity of the event).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsmlEventType">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Used to indicate an event type (the context of the event).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusApplicationName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The unique GLOBUS name of an application.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusVersionName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The GLOBUS name of a version.  Applies to GLOBUS applications only.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusEnquiryName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The unique GLOBUS name of an enquiry.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusRoutineName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The unique GLOBUS name of a custom routine.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusTransactionId">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The unique id of a transaction in GLOBUS.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusOfflineReference">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The off-line reference, return when GLOBUS is off-line but configured to queue transactions for
                               processing once the system returns to the on-line state.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusFieldName">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The GLOBUS name of a field in an OFSML message.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="globusOverrideCode">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A specific code returned from GLOBUS representing an override condition.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusErrorCode">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                A specific code returned from GLOBUS representing an error condition.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusFieldValue">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The value of a single field.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
        <xsd:simpleType name="globusColumnLabel">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                The text name of a column.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
        <xsd:simpleType name="ofsml11TransactionStatus">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  The return status from executing a transaction.
                                OK: The transaction was processed and committed to the database.
                                OVERRIDE: The transaction was processed but an override condition exists.
                                OFFLINE: GLOBUS reported it's status as off-line.  Dependent on the configuration of GLOBUS/OFS,
                                the transaction may be queued in a GLOBUS hold queue for processing once the system is back on-line.
                                FAILED: The transaction failed - no updates were committed to the database unless the GTS control specified
                                the transaction should be placed in 'hold'.
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="OK"/>
                        <xsd:enumeration value="OVERRIDE"/>
                        <xsd:enumeration value="OFFLINE"/>
                        <xsd:enumeration value="FAILED"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsml11ProcessingStatus">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  The status of the transaction (the data record), returned from GLOBUS.
                                This status will indicate the state of the business transaction post any processing completed while executing
                                the request.  It represents a summary code, which is further detailed in the record.status field of the transaction.
                                Note that QUEUED and NOT-QUEUED can only be returned when GLOBUS is off-line (running the
                                end of day processing).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="OK"/>
                        <xsd:enumeration value="REVERSED"/>
                        <xsd:enumeration value="NOT-AUTHORISED"/>
                        <xsd:enumeration value="ON-HOLD"/>
                        <xsd:enumeration value="MATURED"/>
                        <xsd:enumeration value="PARTIAL-INPUT"/>
                        <xsd:enumeration value="QUEUED"/>
                        <xsd:enumeration value="NOT-QUEUED"/>
                </xsd:restriction>
        </xsd:simpleType>
        <xsd:simpleType name="ofsml11EnquiryFormat">
                <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                                Deprecated OFSML 1.1 model.  The type of the OFSML response (applies to enquiry services only).  Two 'sub-types' of OFSML documents
                                are available via the GLOBUS Connector, standard and extended.
                                STANDARD-OFSML: Denotes the normal OFSML enquiry (grid-type structure, normalised column definitions in the service response header).
                                EXTENDED-OFSML: Denotes the enhanced OFSML enquiry (grid-type structure, column names additionally de-normalised into the record sets).
                        </xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:token">
                        <xsd:enumeration value="STANDARD-OFSML"/>
                        <xsd:enumeration value="EXTENDED-OFSML"/>
                </xsd:restriction>
        </xsd:simpleType>
</xsd:schema>